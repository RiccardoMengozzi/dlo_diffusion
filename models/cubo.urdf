<?xml version="1.0"?>
<robot name="cube">
  <!-- Frying pan model, Copyright (c) 2016 Oleg Klimov       -->
  <!-- LICENSE: CC-SA                                         -->
  <link name="cube">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="cubo.obj"/>
      </geometry>
    </visual>

    <collision>
      <geometry>
        <mesh filename="cubo.obj"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>

    <inertial>
      <mass value="0.5"/>  <!-- mr^2 = 0.5*0.12^2 = 0.0072 for Z axis, lighter along other axis -->
      <inertia ixx="0.005" ixy="0.0" ixz="0.0" iyy="0.005" iyz="0.0" izz="0.0072"/>
    </inertial>
  </link>

</robot>

